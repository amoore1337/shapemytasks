{
  "name": "shapemytasks_server",
  "version": "0.0.1",
  "description": "Backend for simple task tracker",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "prod": "node index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "gen:model": "sequelize-cli model:generate",
    "gen:migration": "sequelize-cli migration:generate --name $npm_config_name",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "amoore1337",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "apollo-server-express": "^3.11.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "change-case": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^15.5.3",
    "graphql-subscriptions": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.12.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pem": "^1.14.6",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "require-dir": "^1.2.0",
    "sequelize": "^6.6.5",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "sequelize-cli": "^6.2.0"
  }
}
