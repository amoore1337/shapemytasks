{
  "name": "shapemytasks_server",
  "version": "0.0.1",
  "description": "Backend for simple task tracker",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "prod": "node index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "gen:model": "sequelize-cli model:generate",
    "gen:migration": "sequelize-cli migration:generate --name $npm_config_name",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "amoore1337",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@graphql-tools/schema": "^9.0.12",
    "@prisma/client": "^4.8.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "change-case": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.12.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pem": "^1.14.6",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "require-dir": "^1.2.0",
    "sequelize": "^6.28.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.9",
    "@types/nconf": "^0.10.3",
    "@types/node": "^18.11.17",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
