{
  "name": "shapemytasks_server",
  "version": "0.0.1",
  "description": "Backend for simple task tracker",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "lint": "eslint --fix",
    "prod": "node index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "gen:model": "sequelize-cli model:generate",
    "gen:migration": "sequelize-cli migration:generate --name $npm_config_name",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "amoore1337",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "apollo-server-express": "^3.3.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "change-case": "^4.1.2",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "graphql-subscriptions": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.11.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pem": "^1.14.4",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.3",
    "require-dir": "^1.2.0",
    "sequelize": "^6.6.5",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}
